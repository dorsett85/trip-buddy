{
  "name": "trip-buddy-backend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-server-express": "2.12.0",
    "bcrypt": "4.0.1",
    "commander": "5.1.0",
    "common": "file:../common",
    "cookie-parser": "1.4.5",
    "core-js": "3.6.5",
    "exact": "0.8.0",
    "express": "4.17.1",
    "graphql": "15.0.0",
    "graphql-iso-date": "3.6.1",
    "jsonwebtoken": "9.0.0",
    "pg": "8.0.3"
  },
  "scripts": {
    "server": "nodemon --config ./nodemon.json",
    "db": "ts-node -P tsconfig.json db/cli/cli.ts",
    "migrate": "npm run db -- --pg-file db/pgFile.ts migrate",
    "seed": "npm run db -- --pg-file db/pgFile.ts seed",
    "test": "jest",
    "test:watch": "jest --watch",
    "type:check": "tsc --noEmit",
    "update:common": "npm i ../common",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.13.3",
    "@types/bcrypt": "3.0.0",
    "@types/cookie-parser": "1.4.2",
    "@types/graphql-iso-date": "3.3.3",
    "@types/jest": "25.2.1",
    "@types/jsonwebtoken": "8.3.9",
    "@types/node": "13.13.2",
    "@types/pg": "7.14.3",
    "@typescript-eslint/eslint-plugin": "2.29.0",
    "@typescript-eslint/parser": "2.29.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-prettier": "3.1.3",
    "jest": "25.4.0",
    "nodemon": "2.0.3",
    "prettier": "2.0.5",
    "ts-jest": "25.4.0",
    "ts-node": "8.9.0",
    "typescript": "3.8.3",
    "@graphql-codegen/typescript": "1.13.3",
    "@graphql-codegen/typescript-resolvers": "1.13.3"
  }
}
